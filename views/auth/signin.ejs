<div class="container">
    
    <form class="signin-form" method="POST"  action="/auth/signin">
        <div id="login-header">
            <h3>LOG INTO YOUR ACCOUNT</h3>
        </div>
        <div>
            <label class="signin-label">Email</label>
            <input class="signin-input" type="email" name="email">
        </div>
        <div>
            <label class="signin-label">Password</label>
            <input class="signin-input" type="password" name="password">
        </div>
        <div id="sign-in-submit">
            <input class="login-submit" type="submit" value="Login">
            <br /><br />OR
            <a href="#"><p>Sign in with Google</p></a>
        </div>
    </form>

</div>

